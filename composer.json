{
    "name": "mpprogramming/dto",
    "version": "1.0.2",
    "description": "Package to create dto",
    "type": "library",
    "keywords": [
        "dto",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "MPProgramming",
            "email": "mp.programming007@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MPProgramming\\Dto\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MPProgramming\\Dto\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "=>8.0",
        "illuminate/support": "=>8.0"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "lint": "./vendor/bin/pint"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MPProgramming\\Dto\\DtoServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": ">=9.5",
        "orchestra/testbench": "=>7.5",
        "laravel/pint": "=>0.1.5"
    }
}